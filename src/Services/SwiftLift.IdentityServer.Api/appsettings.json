{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning",
        "Polly": "Warning"
      }
    }
  },
  "AzureLogStream": {
    "Enabled": false
  },
  "AllowedHosts": "*"
}
